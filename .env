# URL Backend API (misalnya untuk autentikasi, refresh token, dll.)
REACT_APP_API_URL=http://localhost:8000

# Secret keys atau API keys (JANGAN taruh informasi rahasia di client-side production build)
REACT_APP_AUTH_CLIENT_ID=your-client-id
REACT_APP_AUTH_CLIENT_SECRET=your-client-secret

# Redirect URL setelah login (terutama untuk aplikasi berbasis OAuth atau OpenID Connect)
REACT_APP_REDIRECT_URI=http://localhost:3000/callback

# URL untuk logout endpoint
REACT_APP_LOGOUT_URL=https://api.example.com/logout

# Expiration time untuk refresh token
REACT_APP_REFRESH_TOKEN_EXPIRY=3600 # in seconds

# Mode aplikasi (misalnya development, production)
REACT_APP_ENV=development

# Token JWT default (untuk testing, sebaiknya kosongkan di production)
REACT_APP_JWT_TOKEN=""

# Client domain (untuk mengatur CORS pada backend)
REACT_APP_CLIENT_DOMAIN=http://localhost:3000
